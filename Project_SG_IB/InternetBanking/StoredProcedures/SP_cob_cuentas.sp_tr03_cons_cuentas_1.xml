  <queries>
    <query>
      <columns>
        <column name="{1}" />
      </columns>
      <queryText>SELECT o_num_filas</queryText>
    </query>
    <query>
      <columns>
        <column name="CODIGO" />
        <column name="CUENTA" />
        <column name="MONEDA" />
        <column name="NOMBRE CUENTA" />
        <column name="OFICIAL" />
        <column name="NOMBREPRODUCTO" />
        <column name="ABREVIACIONPRODUCTO" />
      </columns>
      <queryText>SELECT  "CODIGO" = ta_cliente,"CUENTA" = ta_codigo,"MONEDA" = tt_moneda,"NOMBRE CUENTA" = ta_nombre_tarjeta,"OFICIAL" = ta_oficial </queryText>
    </query>
    <query>
      <columns>
        <column name="CUENTA" />
        <column name="NOMBRE CUENTA" />
      </columns>
      <queryText>SELECT  "CUENTA" = ta_codigo,"NOMBRE CUENTA" = ta_nombre_tarjeta </queryText>
    </query>
    <query>
      <columns>
        <column name="CODIGO" />
        <column name="CUENTA" />
        <column name="MONEDA" />
        <column name="NOMBRE CUENTA" />
        <column name="OFICIAL" />
      </columns>
      <queryText>SELECT  "CODIGO" = ta_cliente,"CUENTA" = ta_codigo,"MONEDA" = 0,"NOMBRE CUENTA" = ta_nombre_tarjeta,"OFICIAL" = ta_oficial </queryText>
    </query>
    <query>
      <columns>
        <column name="CUENTA" />
        <column name="NOMBRE CUENTA" />
      </columns>
      <queryText>SELECT  "CUENTA" = ta_codigo,"NOMBRE CUENTA" = ta_nombre_tarjeta </queryText>
    </query>
    <query>
      <columns>
        <column name="CODIGO" />
        <column name="PRESTAMO" />
        <column name="MONEDA" />
        <column name="TIPO PRESTAMO" />
        <column name="ALIAS" />
      </columns>
      <queryText>SELECT  "CODIGO" = op_operacion,"PRESTAMO" = op_banco,"MONEDA" = op_moneda,"TIPO PRESTAMO" = SUBSTRING (valor, 1, 45),"ALIAS" = op_nombre </queryText>
    </query>
    <query>
      <columns>
        <column name="CODIGO" />
        <column name="DEPOSITO" />
        <column name="MONEDA" />
        <column name="NOMBRE" />
      </columns>
      <queryText>SELECT  "CODIGO" = op_operacion,"DEPOSITO" = op_num_banco,"MONEDA" = op_moneda,"NOMBRE" = op_descripcion </queryText>
    </query>
