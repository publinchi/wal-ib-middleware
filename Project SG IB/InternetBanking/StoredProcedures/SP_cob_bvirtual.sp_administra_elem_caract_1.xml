<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_bvirtual</database>
  <name>sp_administra_elem_caract</name>
  <type>SQLSERVER</type>
  <version>1</version>
  <parameters>
    <parameter name="@t_show_version" dataType="bit" direction="in" />
    <parameter name="@s_date" dataType="datetime" direction="in" />
    <parameter name="@s_user" dataType="varchar" direction="in" />
    <parameter name="@s_ofi" dataType="smallint" direction="in" />
    <parameter name="@s_term" dataType="varchar" direction="in" />
    <parameter name="@s_rol" dataType="smallint" direction="in" />
    <parameter name="@s_srv" dataType="varchar" direction="in" />
    <parameter name="@s_ssn_branch" dataType="int" direction="in" />
    <parameter name="@s_servicio" dataType="tinyint" direction="in" />
    <parameter name="@s_culture" dataType="varchar" direction="in" />
    <parameter name="@t_trn" dataType="int" direction="in" />
    <parameter name="@t_debug" dataType="char" direction="in" />
    <parameter name="@t_file" dataType="varchar" direction="in" />
    <parameter name="@t_from" dataType="varchar" direction="in" />
    <parameter name="@i_operacion" dataType="char" direction="in" />
    <parameter name="@i_le_id" dataType="int" direction="in" />
    <parameter name="@i_lte_codigo" dataType="varchar" direction="in" />
    <parameter name="@i_lc_codigo" dataType="varchar" direction="in" />
    <parameter name="@i_lv_codigo" dataType="varchar" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="ELEMENTO_ID" />
        <column name="TIPO_ELEMENTO_CARACTERISTICA_ID" />
        <column name="CARACTERISTICA_ID" />
        <column name="CARACTERISTICA" />
        <column name="VALOR_CARACTERISTICA_ID" />
        <column name="VALOR_CARACTERISTICA" />
      </columns>
      <queryText>SELECT  "ELEMENTO_ID" = le_id,"TIPO_ELEMENTO_CARACTERISTICA_ID" = lte_codigo,"CARACTERISTICA_ID" = lc_codigo,"CARACTERISTICA" = (SELECT  ISNULL (re_valor, valor) FROM cobis..cl_catalogo c  ,cobis..cl_tabla t  ,cobis..ad_catalogo_i18n   WHERE t.tabla = "ch_tipos_elementos" AND c.tabla = t.codigo AND c.codigo = lc_codigo AND c.estado = "V" AND t.tabla *= pc_identificador AND c.codigo *= pc_codigo AND re_cultura = ?     ),"VALOR_CARACTERISTICA_ID" = lv_codigo,"VALOR_CARACTERISTICA" = (SELECT  ISNULL (re_valor, valor) FROM cobis..cl_catalogo c  ,cobis..cl_tabla t  ,cobis..ad_catalogo_i18n   WHERE t.tabla = "ch_caracteristicas" AND c.tabla = t.codigo AND c.codigo = lv_codigo AND c.estado = "V" AND t.tabla *= pc_identificador AND c.codigo *= pc_codigo AND re_cultura = ?     ) </queryText>
    </query>
  </queries>
</procedure>