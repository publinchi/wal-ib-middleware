<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_procesador</database>
  <name>sp_encryptData</name>
  <type>SYBASE</type>
  <version>1</version>
  <parameters>
    <parameter name="@i_external_customer_id" dataType="int" direction="in" />
    <parameter name="@i_password" dataType="varchar" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="SUCCESS" />
        <column name="PASSWORD" />
        <column name="MESSAGE" />
        <column name="CODE" />
      </columns>
      <queryText>SELECT  "SUCCESS" = en_ente_mis,"PASSWORD" = en_nombre,"MESSAGE" = en_pnombre,"CODE" = en_papellido </queryText>
    </query>
  </queries>
</procedure>