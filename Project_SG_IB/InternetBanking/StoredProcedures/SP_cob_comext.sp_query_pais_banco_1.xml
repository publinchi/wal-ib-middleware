<procedure>
  <database>cob_comext</database>
  <name>sp_query_pais_banco</name>
  <type>SYBASE</type>
  <version>1</version>
  <parameters>
    <parameter name="@i_banco" dataType="smallint" direction="in" />
    <parameter name="@i_pais" dataType="smallint" direction="in" />
    <parameter name="@i_modo" dataType="tinyint" direction="in" />
    <parameter name="@i_est" dataType="char" direction="in" />
    <parameter name="@t_orig_db_name" dataType="varchar" direction="in" />
    <parameter name="@t_orig_sp_name" dataType="varchar" direction="in" />
    <parameter name="@t_orig_trn" dataType="int" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="CODIGO" />
        <column name="NOMBRE DEL BANCO" />
        <column name="CONVENIO" />
        <column name="BANCO_ENTE" />
      </columns>
      <queryText>select 'CODIGO'   = bc_banco,   'NOMBRE DEL BANCO'= substring(bc_descripcion, 1, 42),   'CONVENIO'      = bc_convenio,   'BANCO_ENTE'      = bc_banco_ente   from</queryText>
    </query>
