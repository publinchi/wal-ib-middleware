<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_bvirtual</database>
  <name>sp_busca_bancos</name>
  <type>SQLSERVER</type>
  <version>1</version>
  <parameters>
    <parameter name="@i_operacion" dataType="char" direction="in" />
    <parameter name="@i_banco" dataType="int" direction="in" />
    <parameter name="@i_filial" dataType="tinyint" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="BANCO" />
        <column name="DESCRIPCION" />
        <column name="RUTA-TRANSITO" />
      </columns>
      <queryText>select  'BANCO'            = br_banco,  'DESCRIPCION'      = substring(br_desc,1, 40),        'RUTA-TRANSITO'    = substring(br_ruta_trans, 1, 20)    from</queryText>
    </query>
  </queries>
</procedure>