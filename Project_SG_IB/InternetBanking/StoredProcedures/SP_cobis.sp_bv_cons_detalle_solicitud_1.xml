<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cobis</database>
  <name>sp_bv_cons_detalle_solicitud</name>
  <type>SYBASE</type>
  <version>1</version>
  <parameters>
    <parameter name="@i_tipo_solicitud" dataType="varchar" direction="in" />
    <parameter name="@i_cta" dataType="varchar" direction="in" />
    <parameter name="@i_chequera" dataType="smallint" direction="in" />
    <parameter name="@i_id" dataType="int" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="ID" />
        <column name="CUENTA" />
        <column name="MONTO" />
        <column name="TIPO_CHEQUERA" />
        <column name="TOTAL_CHEQUES" />
        <column name="OFICINA_ENTREGA" />
        <column name="PROPOSITO" />
        <column name="BENEFICIARIO" />
        <column name="ID_AUTORIZADO" />
        <column name="AUTORIZADO" />
        <column name="ESTADO" />
        <column name="TIPO" />
        <column name="SUBTIPO" />
        <column name="PLAZO" />
        <column name="FECHA_FIN" />
        <column name="GARANTIZAR" />
        <column name="TIPO_AVAL" />
        <column name="AVAL" />
      </columns>
      <queryText>SELECT  "CUENTA" = cta.cc_cta_banco,"TIPO CHEQUERA" = tc.tc_nombre,"TOTAL CHEQUES" = ch_numero,"SUC. ENTREGA" = (SELECT  valor FROM cobis..cl_catalogo   WHERE tabla = ? AND CONVERT (SMALLINT,codigo ) = cc.ch_ofi_entrega     ),"ESTADO" = (SELECT  valor FROM cobis..cl_catalogo   WHERE tabla = ? AND codigo = cc.ch_estado     ) </queryText>
    </query>
  </queries>
</procedure>