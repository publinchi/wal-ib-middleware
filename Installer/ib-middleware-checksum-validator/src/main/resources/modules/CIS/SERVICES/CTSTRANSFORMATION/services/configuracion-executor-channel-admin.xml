<?xml version="1.0" encoding="UTF-8"?>
<csp-config name="coreBanking" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="configuracion-transaccion.xsd">
	<!-- Configuracion para ruteo de la operacion -->
	<config priority="1" idzone="routingOrchestrator" id="configuracion ruteo para orquestador sincrono transaccion 18000">
		<conditions>
			<condition param="trn">
				<value>18000</value>
			</condition>
		</conditions>
		<indicators>
			<indicator name="externalProvider" value="0" />
			<indicator name="serviceMethodName" value="executeTransaction" />
			<indicator name="com.cobiscorp.cobis.csp.services.IOrchestrator" value="(service.identifier=spExecutorChannelAdmin)" />
		</indicators>
		<mappings keepMode="1">
		</mappings>
	</config>
</csp-config>
