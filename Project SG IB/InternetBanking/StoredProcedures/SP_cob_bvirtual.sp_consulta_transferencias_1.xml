<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_bvirtual</database>
  <name>sp_consulta_transferencias</name>
  <type>SQLSERVER</type>
  <version>1</version>
  <parameters>
    <parameter name="@i_cuenta" dataType="varchar" direction="in" />
    <parameter name="@i_fecha_ini" dataType="datetime" direction="in" />
    <parameter name="@i_fecha_fin" dataType="datetime" direction="in" />
    <parameter name="@i_formato_fecha" dataType="smallint" direction="in" />
    <parameter name="@i_tipo" dataType="char" direction="in" />
    <parameter name="@i_secuencial" dataType="int" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="FECHA" />
        <column name="CODIGO TIPO PRODUCTO" />
        <column name="TIPO PRODUCTO" />
        <column name="NUMERO PRODUCTO" />
        <column name="BANCO DESTINO" />
        <column name="MONTO" />
        <column name="PROPOSITO" />
        <column name="HORA" />
        <column name="SECUENCIAL" />
        <column name="BENEFICIARIO" />
        <column name="CI BENEFICIARIO" />
        <column name="TELEFONO BENEFICIARIO" />
        <column name="FACTURA" />
        <column name="CURRENCY ID" />
      </columns>
      <queryText>SELECT  "FECHA" = CONVERT (CHAR (10),pa_fecha_pago ,?),"TIPO PRODUCTO" = pa_producto_cr,"NUMERO PRODUCTO" = pa_cuenta_cr,"BANCO DESTINO" = pa_bco_destino,"MONTO" = pa_valor,"PROPOSITO" = pa_concepto,"SECUENCIAL" = pa_id </queryText>
    </query>
    <query>
      <columns>
        <column name="FECHA" />
        <column name="CUENTA CREDITO" />
        <column name="BENEFICIARIO" />
        <column name="MONTO" />
        <column name="CONCEPTO" />
        <column name="HORA" />
        <column name="SECUENCIAL" />
      </columns>
      <queryText>SELECT  "FECHA" = CONVERT (CHAR (10),pa_fecha_pago ,?),"CUENTA CREDITO" = pa_cuenta_cr,"BENEFICIARIO" = pa_beneficiario,"MONTO" = pa_valor,"CONCEPTO" = pa_concepto,"SECUENCIAL" = pa_id </queryText>
    </query>
  </queries>
</procedure>