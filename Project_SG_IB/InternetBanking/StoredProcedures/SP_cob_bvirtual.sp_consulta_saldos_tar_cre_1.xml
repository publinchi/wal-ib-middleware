<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_bvirtual</database>
  <name>sp_consulta_saldos_tar_cre</name>
  <type>SYBASE</type>
  <version>1</version>
  <parameters>
    <parameter name="@s_cliente" dataType="int" direction="in" />
    <parameter name="@s_servicio" dataType="tinyint" direction="in" />
    <parameter name="@s_date" dataType="datetime" direction="in" />
    <parameter name="@s_ofi" dataType="smallint" direction="in" />
    <parameter name="@s_culture" dataType="varchar" direction="in" />
    <parameter name="@t_trn" dataType="int" direction="in" />
    <parameter name="@i_tipo_operacion" dataType="char" direction="in" />
    <parameter name="@t_show_version" dataType="bit" direction="in" />
    <parameter name="@i_prod" dataType="int" direction="in" />
    <parameter name="@i_mon" dataType="int" direction="in" />
    <parameter name="@i_login" dataType="varchar" direction="in" />
    <parameter name="@i_cta" dataType="varchar" direction="in" />
    <parameter name="@i_sec" dataType="int" direction="in" />
    <parameter name="@i_tipo" dataType="char" direction="in" />
    <parameter name="@i_fecha_ini" dataType="datetime" direction="in" />
    <parameter name="@i_fecha_fin" dataType="datetime" direction="in" />
    <parameter name="@i_formato_fecha" dataType="int" direction="in" />
    <parameter name="@i_nro_registros" dataType="int" direction="in" />
    <parameter name="@i_sec_unico" dataType="int" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="productNumber" />
        <column name="productId" />
        <column name="currencyName" />
        <column name="productName" />
        <column name="accountName" />
        <column name="accountingBalance" />
        <column name="drawBalance" />
        <column name="availableBalance" />
        <column name="capitalBalance" />
        <column name="expirationDate" />
        <column name="rate" />
        <column name="capitalBalanceMaturity" />
        <column name="monthlyPaymentDay" />
        <column name="status" />
        <column name="nextPaymentDate" />
        <column name="nextPaymentValue" />
        <column name="totalBalance" />
        <column name="productType" />
        <column name="alias" />
        <column name="currencyId" />
      </columns>
      <queryText>SELECT  "productNumber" = ep_cuenta,"productId" = ep_producto,"currencyName" = ep_cuenta,"productName" = ep_alias,"accountName" = ep_alias,"accountingBalance" = sa_monto_aut,"drawBalance" = sa_saldo,"availableBalance" = sa_monto_uti,"capitalBalance" = ISNULL (sa_monto_uti, 0),"expirationDate" = CONVERT (CHAR (10),ep_fecha_reg ,103),"rate" = 0,"capitalBalanceMaturity" = 0,"monthlyPaymentDay" = 0,"status" = "","nextPaymentDate" = CONVERT (CHAR (10),ep_fecha_mod ,103),"nextPaymentValue" = 0,"totalBalance" = ISNULL (sa_monto_aut, 0),"productType" = ep_producto,"alias" = ep_alias,"currencyId" = ep_moneda </queryText>
    </query>
    <query>
      <columns>
        <column name="MOVIMIENTOS" />
      </columns>
      <queryText>SELECT "MOVIMIENTOS"=w_number_movements</queryText>
    </query>
    <query>
      <columns>
        <column name="FECHA" />
        <column name="TRANSACCION" />
        <column name="COD TRAN" />
        <column name="REFERENCIA" />
        <column name="D/C" />
        <column name="VALOR" />
        <column name="CONTABLE" />
        <column name="DISPONIBLE" />
        <column name="SECUENCIAL" />
        <column name="COD ALTERNO" />
        <column name="HORA" />
        <column name="SEC UNICO" />
        <column name="IMAGEN" />
      </columns>
      <queryText>SELECT  "FECHA" = ep_fecha_reg,"TRANSACCION" = sa_cod_linea,"COD TRAN" = sa_cod_linea,"REFERENCIA" = ep_login,"D/C" = "D","VALOR" = sa_monto_aut,"CONTABLE" = sa_saldo,"DISPONIBLE" = sa_monto_uti,"SECUENCIAL" = sa_cod_linea,"COD ALTERNO" = sa_cod_linea,"HORA" = "","SEC UNICO" = sa_cod_linea,"IMAGEN" = "" </queryText>
    </query>
  </queries>
</procedure>