<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_bvirtual</database>
  <name>sp_cons_colectores_disp_itf</name>
  <type>SQLSERVER</type>
  <version>1</version>
  <parameters>
    <parameter name="@s_cliente" dataType="int" direction="in" />
    <parameter name="@s_servicio" dataType="tinyint" direction="in" />
    <parameter name="@i_operacion" dataType="char" direction="in" />
    <parameter name="@i_colector" dataType="int" direction="in" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="COD." />
        <column name="NOMBRE" />
        <column name="CUENTA" />
        <column name="PRODUCTO" />
        <column name="MONEDA" />
        <column name="DESC MONEDA" />
        <column name="DESC PRODUCTO" />
      </columns>
      <queryText>SELECT  "COD." = co_colector,"NOMBRE" = co_nombre,"CUENTA" = co_cuenta,"PRODUCTO" = co_producto,"MONEDA" = co_moneda,"DESC MONEDA" = mo_descripcion,"DESC PRODUCTO" = pd_descripcion </queryText>
    </query>
  </queries>
</procedure>