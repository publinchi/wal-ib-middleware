<csp-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="coreBanking" xsi:noNamespaceSchemaLocation="configuracion-transaccion.xsd">
	<!--  Configuracion para ruteo de la operacion 19500111 Orquestación Para consulta de cuentas de gastos  -->
	<config priority="1" idzone="routingOrchestrator" id="configuracion ruteo para orquestador sincrono transaccion 19500111">
		<conditions>
			<condition param="trn">
				<value>19500111</value>
				<!-- 19500111 - Consulta Cuentas gastos -->
			</condition>
		</conditions>
		<indicators>
			<indicator name="externalProvider" value="0"/>
			<indicator name="serviceMethodName" value="executeTransaction"/>
			<indicator name="com.cobiscorp.cobis.csp.services.IOrchestrator" value="(service.identifier=ExpensesAccountsOrchestrationCore)"/>
			<indicator name="t_corr" value=""/>
		</indicators>
		<mappings keepMode="1"/>
	</config>
	<!--  Configuracion para ruteo de la operacion 1870042 Orquestación Para Cobro por Solicitud de Tarjetas de Cuentas de Gastos  -->
	<config priority="1" idzone="routingOrchestrator" id="configuracion ruteo para orquestador sincrono transaccion 1870042">
		<conditions>
			<condition param="trn">
				<value>1870042</value>
				<!-- 1870042 - Cobro por Solicitud de Tarjetas de Cuentas de Gastos -->
			</condition>
		</conditions>
		<indicators>
			<indicator name="externalProvider" value="0"/>
			<indicator name="serviceMethodName" value="executeTransaction"/>
			<indicator name="com.cobiscorp.cobis.csp.services.IOrchestrator" value="(service.identifier=RequestCardOrchestrationCore)"/>
			<indicator name="t_corr" value=""/>
		</indicators>
		<mappings keepMode="1"/>
	</config>
</csp-config>