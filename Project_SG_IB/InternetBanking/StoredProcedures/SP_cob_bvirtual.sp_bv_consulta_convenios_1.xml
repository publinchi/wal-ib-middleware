      <queryText>SELECT  "CODIGO CONVENIO" = cv_convenio,"NOMBRE CONVENIO" = cv_nombre,"NEMONICO CONVENIO" = cv_nemonico,"DESCRIPCION CONVENIO" = cv_descripcion,"ESTADO CONVENIO" = cv_estado,"CLIENTE CONVENIO" = cv_cliente,"NOMBRE LARGO" = en_nomlar,"TIPO INTERFAZ" = cv_tipo_interfaz,"FECHA MOD" = cv_fecha_mod,"USUARIO MOD" = cv_usuario_mod,"ID MONEDA" = cv_moneda,"MONEDA" = re_valor </queryText>
    </query>
