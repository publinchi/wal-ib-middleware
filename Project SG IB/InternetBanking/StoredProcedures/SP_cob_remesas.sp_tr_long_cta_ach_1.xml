      </columns>
      <queryText>select 'BANCO'            = br_banco,  'DESCRIPCION'      = substring(br_desc,1, 40),  'RUTA-TRANSITO'    = substring(br_ruta_trans, 1, 20),  'FILIAL'           = br_filial,  'ESTADO'           = br_estado,  'TIPO DE CUENTA'   = lc_tipo_cta_ach,  'DESC. CUENTA'     = substring(valor,1,20),  'LONGITUD CUENTA'  = lc_long_cta    from</queryText>
    </query>
  </queries>
</procedure>