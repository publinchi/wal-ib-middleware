<?xml version="1.0" encoding="utf-8"?>
<procedure>
  <database>cob_bvirtual</database>
  <name>sp_get_all_quest_api</name>
  <type>SQLSERVER</type>
  <version>1</version>
  <parameters>
    <parameter name="@i_customer" dataType="int" direction="in" />
    <parameter name="@o_success" dataType="bit" direction="out" />
    <parameter name="@o_code" dataType="int" direction="out" />
    <parameter name="@o_message" dataType="varchar" direction="out" />
  </parameters>
  <queries>
    <query>
      <columns>
        <column name="question_id" />
        <column name="question_description" />
      </columns>
      <queryText>SELECT  "question_id" = pr_pregunta,"question_description" = "" </queryText>
    </query>
    <query>
      <columns>
        <column name="question_id" />
        <column name="answer_id" />
        <column name="answer_description" />
      </columns>
      <queryText>SELECT  "question_id" = re_pregunta,"answer_id" = re_respuesta,"answer_description" = re_descripcion </queryText>
    </query>
  </queries>
</procedure>